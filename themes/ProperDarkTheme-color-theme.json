{
	"name": "Proper dark theme",
	"type": "dark",
	"colors": {
		"editorIndentGuide.activeBackground": "#383838",
		"editorIndentGuide.background": "#2A2A2A",
		"editorRuler.foreground": "#2A2A2A",
		"editorLineNumber.foreground": "#d0d0d0",
		"activityBar.background": "#242424",
		"activityBar.foreground": "#ffffff",
    "activityBar.border": "#2c2c2c",
		"activityBarBadge.background": "#383838",
		"badge.background": "#383838",
		"badge.foreground": "#C1C1C1",
		"button.background": "#333",
		"editor.background": "#242424",
		"editor.lineHighlightBorder": "#303030",
		"editorGroupHeader.tabsBackground": "#1A1A1A",
		"editorGroupHeader.tabsBorder": "#1A1A1A",
		"editorSuggestWidget.background": "#1A1A1A",
		"focusBorder": "#444",
		"foreground": "#cccccc",
		"input.background": "#2A2A2A",
		"input.foreground": "#E0E0E0",
		"list.activeSelectionBackground": "#3c3c3c",
		"list.activeSelectionForeground": "#ffffff",
		"list.focusBackground": "#505050",
		"list.highlightForeground": "#EAEAEA",
		"list.hoverBackground": "#505050",
		"list.hoverForeground": "#ffffff",
		"list.inactiveSelectionBackground": "#222222",
		"list.inactiveSelectionForeground": "#ffffff",
    "list.invalidItemForeground": "#ffffff",
    "panel.background": "#242424",
		"panelTitle.activeBorder": "#505050",
		"panelTitle.activeForeground": "#FAFAFA",
		"panelTitle.inactiveForeground": "#888888",
		"peekView.border": "#444",
		"peekViewEditor.background": "#242424",
		"pickerGroup.border": "#363636",
		"pickerGroup.foreground": "#EAEAEA",
		"progressBar.background": "#FAFAFA",
		"scrollbar.shadow": "#1f1f1f",
		"sideBar.background": "#222222",
    "sideBar.border": "#313131",
		"sideBarSectionHeader.background": "#2b2b2b",
		"statusBar.background": "#2e2e2e",
		"statusBar.debuggingBackground": "#af3131",
    "statusBar.debuggingForeground": "#ffffff",
		"statusBar.foreground": "#d8d5d5",
		"statusBar.noFolderBackground": "#1A1A1A",
		"statusBarItem.remoteForeground": "#ffffff",
		"statusBarItem.remoteBackground": "#0070a3",
		"statusBarItem.prominentBackground": "#fafafa1a",
		"tab.activeBorder": "#1e1e1e",
		"tab.activeBackground": "#353535",
		"tab.activeForeground": "#FAFAFA",
		"tab.border": "#1A1A1A",
		"tab.inactiveBackground": "#1A1A1A",
		"tab.inactiveForeground": "#727272",
		"textLink.foreground": "#CCC",
		"textLink.activeForeground": "#ffffff",
		"titleBar.activeBackground": "#242424",
    "titleBar.inactiveBackground": "#242424",
    "titleBar.inactiveForeground": "#ffffff",
    "titleBar.border": "#2c2c2c",
    "menu.background": "#2e2e2e",
    "menu.foreground": "#ffffff",
    "terminal.background": "#242424",
    "terminal.border": "#353535",
		"terminal.ansiBrightBlack": "#222121",
		"inputOption.activeBackground": "#2b2b2b",
		"debugIcon.continueForeground": "#79b8ff",
		"debugIcon.disconnectForeground": "#79b8ff",
		"debugIcon.pauseForeground": "#FF7A84",
		"debugIcon.restartForeground": "#79b8ff",
		"debugIcon.startForeground": "#24f324",
		"debugIcon.stepBackForeground": "#79b8ff",
		"debugIcon.stepIntoForeground": "#24f324",
		"debugIcon.stepOutForeground": "#79b8ff",
		"debugIcon.stepOverForeground": "#24f324",
		"debugIcon.stopForeground": "#79b8ff",
		"debugIcon.breakpointCurrentStackframeForeground": "#79b8ff",
		"debugIcon.breakpointDisabledForeground": "#848484",
		"debugIcon.breakpointForeground": "#FF7A84",
		"debugIcon.breakpointStackframeForeground": "#79b8ff",
		"debugIcon.breakpointUnverifiedForeground": "#848484",
		"symbolIcon.classForeground": "#FF9800",
		"symbolIcon.enumeratorForeground": "#FF9800",
		"symbolIcon.eventForeground": "#FF9800",
		"symbolIcon.methodForeground": "#b392f0",
		"symbolIcon.constructorForeground": "#b392f0",
		"symbolIcon.functionForeground": "#b392f0",
		"symbolIcon.fieldForeground": "#79b8ff",
		"symbolIcon.interfaceForeground": "#79b8ff",
		"symbolIcon.variableForeground": "#79b8ff",
		"symbolIcon.enumeratorMemberForeground": "#79b8ff",
		"diffEditor.insertedTextBackground": "#3a632a4b",
		"diffEditor.removedTextBackground": "#b3034752",
	},
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#e5e9ed"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#d2a8ff"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#e1e4e8"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#78b4e9"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#b2dbff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#c7e6ff"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#bfdef8"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c9d1d9"
      }
    },
    {
    "scope": ["storage.type.java", 
        "variable.other.object.java"],
      "settings": {
        "foreground": "#28b7ff"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#414344",
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#d2a8ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#a5d6ff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.object.property", 
        "variable.object.property.js", 
        "variable.object.property.ts"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "string.quoted.double",
        "string.quoted.double.json",
        "string.quoted.double.js",
        "string.quoted.double.ts",
        "string.quoted.double.tsx",
        "string.quoted.double.java",
        "string.quoted.double.python",
      ],
      "settings": {
        "foreground": "#94cfff"
      }
    },
    {
      "scope": "entity.other",
      "settings": {
        "foreground": "#be90ff"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#f2ebeb"
      }
    },
    {
      "scope": "source.ignore",
      "settings": {
        "foreground": "#d6d6d6"
      }
    },
    {
      "scope": "support.type.property-name.",
      "settings": {
        "foreground": "#73b7ff"
      }
    }
  ]
}
