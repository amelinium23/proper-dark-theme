    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#86bfff"
      }
    },
	{
		"scope": [
			"variable.object.property.js",
			"meta.definition.property.js",
			"meta.field.declaration.js",
			"source.js",
		],
		"settings": {
			"foreground": "#2cc0ff",
		}
	},
  {
    "scope": "support.function.js",
    "settings": {
      "foreground": "#79b8ff"
    }
  },
  {
    "scope": "meta.jsx.children.js",
